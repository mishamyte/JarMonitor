{
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console"
        ],
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "outputTemplate": "{Timestamp:dd-MM-yyyy HH:mm:ss} [{Level:u3}] {Message}{NewLine}{Exception}"
                }
            }
        ]
    },
    "Timezone": "Europe/Kyiv",
    "ScheduleTime": "23:59",
    "Jars": [
        {
            "Name": "SAMPLE_NAME",
            "JarId": "SAMPLE_VALUE"
        }
    ],
    "Telegram": {
        "BotToken": "BOT_TOKEN",
        "ChannelId": "CHANNEL_ID"
    }
}
